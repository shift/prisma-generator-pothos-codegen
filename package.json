{
  "name": "prisma-generator-pothos-codegen",
  "version": "0.5.8",
  "license": "MIT",
  "description": "A prisma generator that generate input types for Pothos. And optionally generate all crud.",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./src/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cauen/prisma-generator-pothos-codegen.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Emanuel CauÃª",
  "keywords": [
    "pothos",
    "graphql",
    "typescript",
    "prisma",
    "db",
    "postgres",
    "codegen"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "tscheck": "tsc -noEmit",
    "prebuild": "yarn clean",
    "postbuild": "shx cp ./README.md ./dist && shx chmod +x ./dist/src/bin.js",
    "clean": "shx rm -rf dist",
    "pub": "cd dist && npm publish",
    "format": "eslint --fix ./src"
  },
  "bin": {
    "prisma-generator-pothos-codegen": "./src/bin.js"
  },
  "peerDependencies": {
    "@pothos/core": "^3.23.0",
    "@pothos/plugin-prisma": "^3.37.0",
    "@prisma/client": "^4.7.0",
    "graphql": "^16.6.0",
    "prisma": "^4.16.2"
  },
  "devDependencies": {
    "@pothos/core": "3.41.2",
    "@pothos/plugin-prisma": "3.65.3",
    "@prisma/client": "4.16.2",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.19",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "4.2.1",
    "graphql": "16.9.0",
    "jest": "^28.1.3",
    "prettier": "3.3.3",
    "prisma": "4.16.2",
    "shx": "0.3.4",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "@prisma/generator-helper": "4.16.2",
    "@prisma/internals": "4.16.2",
    "tokenizr": "1.7.0"
  }
}
